    /**
     * @var \VendorBundle\Entity\MCountry
     *
     * @ORM\ManyToOne(targetEntity="VendorBundle\Entity\MCountry")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="COUNTRY_ID", referencedColumnName="ID")
     * })
     */
    private $country;


}

