    <id name="id" type="bigint" column="ID">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="PERUSAHAAN_ID_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="alamat" type="string" column="ALAMAT" length="2000" nullable="true">
      <options>
